[run]
branch = True

[paths]
source =
    auto_round/
    */site-packages/auto_round/
    */dist-packages/auto_round/

[report]
include =
 */auto_round/**
 */auto_round_extension/**
exclude_lines =
 pragma: no cover
 raise NotImplementedError
 raise TypeError
 except ImportError:
 except Exception as e: